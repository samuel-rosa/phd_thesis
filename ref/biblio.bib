@Article{GallantEtAl1996,
  Title                    = {TAPES-G: A grid-based terrain analysis program for the environmental sciences},
  Author                   = {John C. Gallant and John P. Wilson},
  Journal                  = {Computers and Geosciences},
  Pages                    = {713-722},
  Volume                   = {22},
